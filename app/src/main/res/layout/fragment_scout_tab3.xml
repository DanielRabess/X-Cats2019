<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ScoutTab3">

    <!-- TODO: Update blank fragment layout -->


    <TextView
        android:id="@+id/controlTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/control"
        android:textSize="30sp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="41dp"
        tools:layout_conversion_absoluteWidth="99dp" />

    <android.support.constraint.Guideline
        android:id="@+id/centerVerticalGuideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <android.support.constraint.Guideline
        android:id="@+id/leftShipQuarterGuideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.25" />

    <android.support.constraint.Guideline
        android:id="@+id/rightShipQuarterGuideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.75" />

    <android.support.constraint.Guideline
        android:id="@+id/topTextHorizontalGuideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />

    <android.support.constraint.Guideline
        android:id="@+id/rocketCargoShipSeparatorHorizontalGuideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.7" />

    <Button
        android:id="@+id/leftShipL3Button"
        android:layout_width="wrap_content"
        android:layout_height="47dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="Level 3"
        app:layout_constraintBottom_toTopOf="@+id/leftShipL2Button"
        app:layout_constraintEnd_toStartOf="@+id/leftShipQuarterGuideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/topTextHorizontalGuideline" />

    <Button
        android:id="@+id/leftShipL2Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Level 2"
        app:layout_constraintBottom_toTopOf="@+id/leftShipL1Button"
        app:layout_constraintEnd_toEndOf="@+id/leftShipL3Button"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/leftShipL3Button"
        app:layout_constraintTop_toBottomOf="@+id/leftShipL3Button" />

    <Button
        android:id="@+id/leftShipL1Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="Level 1"
        app:layout_constraintBottom_toTopOf="@+id/rocketCargoShipSeparatorHorizontalGuideline"
        app:layout_constraintEnd_toEndOf="@+id/leftShipL2Button"
        app:layout_constraintStart_toStartOf="@+id/leftShipL2Button"
        app:layout_constraintTop_toBottomOf="@+id/leftShipL2Button" />


    <Button
        android:id="@+id/rightShipL3Button"
        android:layout_width="wrap_content"
        android:layout_height="47dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="Level 3"
        app:layout_constraintBottom_toTopOf="@+id/rightShipL2Button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rightShipQuarterGuideline"
        app:layout_constraintTop_toTopOf="@+id/topTextHorizontalGuideline" />

    <Button
        android:id="@+id/rightShipL2Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Level 2"
        app:layout_constraintBottom_toTopOf="@+id/rightShipL1Button"
        app:layout_constraintEnd_toEndOf="@+id/rightShipL3Button"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/rightShipL3Button"
        app:layout_constraintTop_toBottomOf="@+id/rightShipL3Button" />

    <Button
        android:id="@+id/rightShipL1Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="Level 1"
        app:layout_constraintBottom_toTopOf="@+id/rocketCargoShipSeparatorHorizontalGuideline"
        app:layout_constraintEnd_toEndOf="@+id/rightShipL2Button"
        app:layout_constraintStart_toStartOf="@+id/rightShipL2Button"
        app:layout_constraintTop_toBottomOf="@+id/rightShipL2Button" />

    <ImageView
        android:id="@+id/cargoCircleLeftL3"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:src="@drawable/smallgraycircle"
        app:layout_constraintBottom_toBottomOf="@+id/leftShipL3Button"
        app:layout_constraintEnd_toStartOf="@+id/hatchCircleLeftL3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/leftShipQuarterGuideline"
        app:layout_constraintStart_toStartOf="@+id/leftShipQuarterGuideline"
        app:layout_constraintTop_toTopOf="@+id/leftShipL3Button" />

    <ImageView
        android:id="@+id/hatchCircleLeftL3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:src="@drawable/smallgraycircle"
        app:layout_constraintBottom_toBottomOf="@+id/cargoCircleLeftL3"
        app:layout_constraintEnd_toStartOf="@+id/centerVerticalGuideline"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/cargoCircleLeftL3"
        app:layout_constraintTop_toTopOf="@+id/cargoCircleLeftL3"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/cargoCircleLeftL2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/smallgraycircle"
        app:layout_constraintBottom_toBottomOf="@+id/leftShipL2Button"
        app:layout_constraintEnd_toEndOf="@+id/cargoCircleLeftL3"
        app:layout_constraintStart_toStartOf="@+id/cargoCircleLeftL3"
        app:layout_constraintTop_toTopOf="@+id/leftShipL2Button" />

    <ImageView
        android:id="@+id/hatchCircleLeftL2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/smallgraycircle"
        app:layout_constraintBottom_toBottomOf="@+id/cargoCircleLeftL2"
        app:layout_constraintEnd_toEndOf="@+id/hatchCircleLeftL3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/hatchCircleLeftL3"
        app:layout_constraintTop_toTopOf="@+id/cargoCircleLeftL2" />

    <ImageView
        android:id="@+id/cargoCircleLeftL1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/smallgraycircle"
        app:layout_constraintBottom_toBottomOf="@+id/leftShipL1Button"
        app:layout_constraintEnd_toEndOf="@+id/cargoCircleLeftL2"
        app:layout_constraintStart_toStartOf="@+id/cargoCircleLeftL2"
        app:layout_constraintTop_toTopOf="@+id/leftShipL1Button" />

    <ImageView
        android:id="@+id/hatchCircleLeftL1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/smallgraycircle"
        app:layout_constraintBottom_toBottomOf="@+id/cargoCircleLeftL1"
        app:layout_constraintEnd_toEndOf="@+id/hatchCircleLeftL2"
        app:layout_constraintStart_toStartOf="@+id/hatchCircleLeftL2"
        app:layout_constraintTop_toTopOf="@+id/cargoCircleLeftL1" />

    <ImageView
        android:id="@+id/hatchCircleRightL1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/smallgraycircle"
        app:layout_constraintBottom_toBottomOf="@+id/cargoCircleRightL1"
        app:layout_constraintEnd_toEndOf="@+id/hatchCircleRightL2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/hatchCircleRightL2"
        app:layout_constraintTop_toTopOf="@+id/cargoCircleRightL1" />

    <ImageView
        android:id="@+id/cargoCircleRightL1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/smallgraycircle"
        app:layout_constraintBottom_toBottomOf="@+id/rightShipL1Button"
        app:layout_constraintEnd_toEndOf="@+id/cargoCircleRightL2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/cargoCircleRightL2"
        app:layout_constraintTop_toTopOf="@+id/rightShipL1Button" />

    <ImageView
        android:id="@+id/hatchCircleRightL3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:src="@drawable/smallgraycircle"
        app:layout_constraintBottom_toBottomOf="@+id/cargoCircleRightL3"
        app:layout_constraintEnd_toStartOf="@+id/cargoCircleRightL3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/centerVerticalGuideline"
        app:layout_constraintTop_toTopOf="@+id/cargoCircleRightL3" />

    <ImageView
        android:id="@+id/cargoCircleRightL3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:src="@drawable/smallgraycircle"
        app:layout_constraintBottom_toBottomOf="@+id/rightShipL3Button"
        app:layout_constraintEnd_toStartOf="@+id/rightShipQuarterGuideline"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/hatchCircleRightL3"
        app:layout_constraintTop_toTopOf="@+id/rightShipL3Button" />

    <ImageView
        android:id="@+id/hatchCircleRightL2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/smallgraycircle"
        app:layout_constraintBottom_toBottomOf="@+id/cargoCircleRightL2"
        app:layout_constraintEnd_toEndOf="@+id/hatchCircleRightL3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/hatchCircleRightL3"
        app:layout_constraintTop_toTopOf="@+id/cargoCircleRightL2"
        app:layout_constraintVertical_bias="0.29000002" />

    <ImageView
        android:id="@+id/cargoCircleRightL2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/smallgraycircle"
        app:layout_constraintBottom_toBottomOf="@+id/rightShipL2Button"
        app:layout_constraintEnd_toEndOf="@+id/cargoCircleRightL3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/cargoCircleRightL3"
        app:layout_constraintTop_toTopOf="@+id/rightShipL2Button" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cargo"
        app:layout_constraintStart_toStartOf="@+id/cargoCircleLeftL3"
        app:layout_constraintTop_toTopOf="@+id/cargoCircleLeftL3"
        app:layout_constraintEnd_toEndOf="@+id/cargoCircleLeftL3"
        app:layout_constraintBottom_toBottomOf="@+id/cargoCircleLeftL3"
        />


</android.support.constraint.ConstraintLayout>